       if hspeed&gt;-gaitmax hspeed-=gait else hspeed=-gaitmax
    } else  
    if inputdog_down("right", playerSlot) and (x + 16 &lt; view_xview[0] + view_wview[0])
    {   //rot=270
         if hspeed&lt;gaitmax hspeed+=gait else hspeed=gaitmax
    } else  if roll=0 {hspeed=0}
}// else speed=0    
    
    if inputdog_pressed("butt1", playerSlot) and sprite_index=sNinjaW and jumped=0 
