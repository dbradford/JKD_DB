{
    if inputdog_down("up", playerSlot) and (y&gt;9)
    {    //rot=0
         if vspeed&gt;-gaitmax vspeed-=gait else vspeed=-gaitmax
    } else
    if inputdog_down("down", playerSlot) and (y+11&lt; room_height)
    {//rot=180
       if vspeed&lt;gaitmax vspeed+=gait else vspeed=gaitmax
    } else if roll=0 {vspeed=0}


    if inputdog_down("left", playerSlot) and (x-10&gt;view_xview[0])
